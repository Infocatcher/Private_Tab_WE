{
	"manifest_version": 2,
	"version": "0.1a1",
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"default_locale": "en",
	"applications": {
		"gecko": {
			"id": "privateTabWE@infocatcher"
		}
	},

	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
	    "contextMenus"
	],
	"browser_action": {
		"browser_style": true,
		"default_title": "__MSG_openNewPrivateTab__",
		"default_icon": {
			"16": "privacy-16.png",
			"24": "privacy-24.png",
			"32": "privacy-32.png"
		}
	},

	"icons": {
		"16": "privacy-16.png",
		"48": "icon.png"
	}
}